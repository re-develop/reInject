<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>ReInject.PostInjectors.BackgroundWorker</RootNamespace>
    <PackageId>Aeolin.reInject.PostInjectors.BackgroundWorker</PackageId>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Aeolin Ferjünnoz</Authors>
    <Company>reDevelop</Company>
    <Product>ReInject.PostInjectors.BackgroundWorker</Product>
    <PackageProjectUrl>https://github.com/re-develop/reInject</PackageProjectUrl>
    <RepositoryUrl>https://github.com/re-develop/reInject</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>di dependency injection background worker scheduler</PackageTags>
    <AssemblyVersion>1.5.3</AssemblyVersion>
    <FileVersion>1.5.3</FileVersion>
    <Copyright>Aeolin Ferjünnoz</Copyright>
    <Description>An extension for reInject adding post injectable background workers</Description>
    <Version>1.5.3</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aeolin.reInject" Version="1.5.3" />
    <PackageReference Include="Cronos" Version="0.7.1" />
  </ItemGroup>

</Project>
